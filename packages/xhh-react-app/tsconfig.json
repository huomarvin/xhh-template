{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@pages": ["./src/pages"],
      "@components": ["./src/components"],
      "@imgs": ["./src/imgs"],
      "@imgs/": ["./src/imgs/"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
